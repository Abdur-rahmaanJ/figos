proginf = "print each line of stdin only once, regardless of order of lines"

function re(z p)
f=z ; split(f p) ; join(f "") ; return(f)
fig
l ; arr
forin (p stdin)
cr=13 ; chr
lf=10 ; chr
z=p ; re(z cr) ; re(z lf)

inl instr l z
iftrue inl

pass
else
p = z ; print
l plus z
fig

next
